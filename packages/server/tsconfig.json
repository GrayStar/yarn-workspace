{
	"compilerOptions": {
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"target": "es5",
		"noImplicitAny": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"sourceMap": true,
		"baseUrl": ".",
		"downlevelIteration": true
	},
	"include": ["src"]
}
